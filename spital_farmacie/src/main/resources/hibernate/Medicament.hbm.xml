<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Medicament"
           table="Medicament">
        <id name="id" column="id_mes" type="integer">
            <generator class="increment"/>
        </id>
        <property
                name="nume"
                column="nume"
                type="string"/>
        <property
                name="cantitate_pe_stoc"
                column="cantitate_pe_stoc"
                type="integer"/>

    </class>
</hibernate-mapping>